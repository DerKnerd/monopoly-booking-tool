<Window x:Class="Monopoly_Booking_Tool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" MinHeight="300" MinWidth="560">
    <Grid>
        <TabControl>
            <TabItem>
                <TabItem.Header>
                    Spielerverwaltung
                </TabItem.Header>
                <Frame Name="playermanagement" NavigationUIVisibility="Hidden" />
            </TabItem>
            <TabItem>
                <TabItem.Header>
                    Straßenverwaltung
                </TabItem.Header>
                <Frame Name="streetmanagement" NavigationUIVisibility="Hidden" />
            </TabItem>
            <TabItem>
                <TabItem.Header>
                    Sonderkartenverwaltung
                </TabItem.Header>
                <Frame Name="extrasmanagement" NavigationUIVisibility="Hidden" />
             </TabItem>
        </TabControl>
    </Grid>
</Window>
